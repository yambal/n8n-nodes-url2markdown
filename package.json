{
  "name": "n8n-nodes-url2markdown",
  "version": "0.2.0",
  "description": "n8n node to convert URL to Markdown using Readability and Turndown",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "markdown",
    "url",
    "web",
    "scraping",
    "readability",
    "turndown"
  ],
  "license": "MIT",
  "author": {
    "name": "jnymmt"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yambal/n8n-nodes-url2markdown.git"
  },
  "main": "dist/nodes/Url2Markdown/Url2Markdown.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/Url2Markdown/Url2Markdown.node.js"
    ],
    "credentials": []
  },
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "node -e \"require('fs').cpSync('src/nodes/Url2Markdown/url2markdown.svg', 'dist/nodes/Url2Markdown/url2markdown.svg')\"",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@mozilla/readability": "^0.5.0",
    "jsdom": "^26.0.0",
    "turndown": "^7.2.0"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.0.0",
    "@types/turndown": "^5.0.5",
    "n8n-workflow": "^1.70.0",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  }
}
