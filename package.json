{
  "name": "n8n-nodes-url2markdown",
  "version": "0.1.0",
  "description": "n8n node to convert URL to Markdown using Jina Reader API",
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "markdown",
    "url",
    "web",
    "scraping",
    "jina"
  ],
  "license": "MIT",
  "author": {
    "name": "jnymmt"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yambal/n8n-nodes-url2markdown.git"
  },
  "main": "dist/nodes/Url2Markdown/Url2Markdown.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/Url2Markdown/Url2Markdown.node.js"
    ],
    "credentials": []
  },
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "node -e \"require('fs').cpSync('src/nodes/Url2Markdown/url2markdown.svg', 'dist/nodes/Url2Markdown/url2markdown.svg')\"",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^22.0.0",
    "n8n-workflow": "^1.70.0",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  }
}
